{
  "name": "markdown-it-cjk-friendly",
  "version": "1.0.3",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module-sync": "./dist/index.js",
      "import": "./dist/index.js",
      "node": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist", "LICENSE", "README.md"],
  "repository": {
    "url": "https://github.com/tats-u/markdown-cjk-friendly"
  },
  "license": "MIT",
  "author": "Tatsunori Uchino <tats.u@live.jp> (https://github.com/tats-u)",
  "bugs": "https://github.com/tats-u/markdown-cjk-friendly/issues",
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "japanese",
    "chinese",
    "korean",
    "cjk"
  ],
  "description": "markdown-it plugin to make Markdown emphasis (`**`) in CommonMark more friendly with Chinese, Japanese, and Korean (CJK)",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "build:lib": "tsup",
    "build:rslib": "rslib build",
    "dev": "tsup --watch",
    "dev:lib": "tsup --watch",
    "dev:rslib": "rslib build --watch",
    "test": "vitest run",
    "test:lib": "vitest run",
    "test:up": "vitest -u",
    "test:watch": "vitest watch",
    "test:lib:watch": "vitest watch",
    "bench": "vitest bench --run",
    "bench:watch": "vitest bench",
    "lint:type": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "markdown-it": "^14.1.0"
  },
  "peerDependencies": {
    "@types/markdown-it": "*",
    "markdown-it": "*"
  },
  "peerDependenciesMeta": {
    "@types/markdown-it": {
      "optional": true
    }
  },
  "dependencies": {
    "get-east-asian-width": "^1.3.0"
  },
  "engines": {
    "node": ">=16"
  }
}
