{
  "name": "markdown-it-cj-friendly",
  "version": "0.1.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "module-sync": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "url": "https://github.com/tats-u/markdown-cj-friendly"
  },
  "license": "MIT",
  "author": "Tatsunori Uchino <tats.u@live.jp> (https://github.com/tats-u)",
  "bugs": "https://github.com/tats-u/markdown-cj-friendly/issues",
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "japanese",
    "chinese"
  ],
  "description": "markdown-it plugin to make Markdown emphasis (`**`) in CommonMark more friendly with Chinese and Japanese",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "build:lib": "tsup",
    "dev:lib": "tsup --watch",
    "build:rslib": "rslib build",
    "dev:rslib": "rslib build --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:lib:watch": "vitest watch",
    "lint:type": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "markdown-it": "^14.1.0"
  },
  "peerDependencies": {
    "@types/markdown-it": "*",
    "markdown-it": "*"
  },
  "peerDependenciesMeta": {
    "@types/markdown-it": {
      "optional": true
    }
  }
}
